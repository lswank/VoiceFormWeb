[
  {
    "id": "form-1",
    "title": "Customer Feedback Survey",
    "description": "Help us improve our products and services by providing your feedback.",
    "fields": [
      {
        "id": "field-1-1",
        "type": "text",
        "label": "Full Name",
        "placeholder": "Enter your full name",
        "required": true
      },
      {
        "id": "field-1-2",
        "type": "email",
        "label": "Email Address",
        "placeholder": "Enter your email address",
        "required": true
      },
      {
        "id": "field-1-3",
        "type": "radio",
        "label": "How satisfied are you with our service?",
        "required": true,
        "options": [
          { "label": "Very Satisfied", "value": "very_satisfied" },
          { "label": "Satisfied", "value": "satisfied" },
          { "label": "Neutral", "value": "neutral" },
          { "label": "Dissatisfied", "value": "dissatisfied" },
          { "label": "Very Dissatisfied", "value": "very_dissatisfied" }
        ]
      },
      {
        "id": "field-1-4",
        "type": "voice",
        "label": "Please explain your rating",
        "placeholder": "Speak your feedback",
        "required": false
      }
    ],
    "settings": {
      "allowAnonymousResponses": true,
      "confirmationMessage": "Thank you for your feedback!",
      "redirectUrl": "",
      "sendConfirmationEmail": true,
      "notifyOnSubmission": true
    },
    "createdAt": "2023-02-01T00:00:00.000Z",
    "updatedAt": "2023-02-01T00:00:00.000Z"
  },
  {
    "id": "form-2",
    "title": "Job Application Form",
    "description": "Apply for open positions at our company.",
    "fields": [
      {
        "id": "field-2-1",
        "type": "text",
        "label": "Full Name",
        "placeholder": "Enter your full name",
        "required": true
      },
      {
        "id": "field-2-2",
        "type": "email",
        "label": "Email Address",
        "placeholder": "Enter your email address",
        "required": true
      },
      {
        "id": "field-2-3",
        "type": "phone",
        "label": "Phone Number",
        "placeholder": "Enter your phone number",
        "required": true
      },
      {
        "id": "field-2-4",
        "type": "select",
        "label": "Position Applying For",
        "required": true,
        "options": [
          { "label": "Software Developer", "value": "developer" },
          { "label": "UI/UX Designer", "value": "designer" },
          { "label": "Product Manager", "value": "product_manager" },
          { "label": "Marketing Specialist", "value": "marketing" },
          { "label": "Customer Support", "value": "support" }
        ]
      },
      {
        "id": "field-2-5",
        "type": "file",
        "label": "Resume/CV",
        "required": true
      },
      {
        "id": "field-2-6",
        "type": "voice",
        "label": "Why do you want to work with us?",
        "placeholder": "Record your response",
        "required": true
      }
    ],
    "settings": {
      "allowAnonymousResponses": false,
      "confirmationMessage": "Thank you for your application! We will review it and get back to you soon.",
      "redirectUrl": "",
      "sendConfirmationEmail": true,
      "notifyOnSubmission": true
    },
    "createdAt": "2023-02-10T00:00:00.000Z",
    "updatedAt": "2023-02-10T00:00:00.000Z"
  },
  {
    "id": "form-3",
    "title": "Event Registration",
    "description": "Register for our upcoming conference and workshops.",
    "fields": [
      {
        "id": "field-3-1",
        "type": "text",
        "label": "Full Name",
        "placeholder": "Enter your full name",
        "required": true
      },
      {
        "id": "field-3-2",
        "type": "email",
        "label": "Email Address",
        "placeholder": "Enter your email address",
        "required": true
      },
      {
        "id": "field-3-3",
        "type": "checkbox-group",
        "label": "Which sessions will you attend?",
        "required": true,
        "options": [
          { "label": "Keynote Speech", "value": "keynote" },
          { "label": "Technical Workshop", "value": "technical" },
          { "label": "Networking Event", "value": "networking" },
          { "label": "Panel Discussion", "value": "panel" }
        ]
      },
      {
        "id": "field-3-4",
        "type": "radio",
        "label": "Meal Preference",
        "required": true,
        "options": [
          { "label": "Regular", "value": "regular" },
          { "label": "Vegetarian", "value": "vegetarian" },
          { "label": "Vegan", "value": "vegan" },
          { "label": "Gluten-Free", "value": "gluten_free" }
        ]
      },
      {
        "id": "field-3-5",
        "type": "voice",
        "label": "Any special accommodations needed?",
        "placeholder": "Speak your requirements",
        "required": false
      }
    ],
    "settings": {
      "allowAnonymousResponses": false,
      "confirmationMessage": "Thank you for registering! We look forward to seeing you at the event.",
      "redirectUrl": "",
      "sendConfirmationEmail": true,
      "notifyOnSubmission": true
    },
    "createdAt": "2023-02-15T00:00:00.000Z",
    "updatedAt": "2023-02-15T00:00:00.000Z"
  },
  {
    "id": "form-4",
    "title": "Product Survey",
    "description": "Help us understand how you use our products.",
    "fields": [
      {
        "id": "field-4-1",
        "type": "text",
        "label": "Name",
        "placeholder": "Enter your name",
        "required": false
      },
      {
        "id": "field-4-2",
        "type": "email",
        "label": "Email",
        "placeholder": "Enter your email",
        "required": false
      },
      {
        "id": "field-4-3",
        "type": "slider",
        "label": "How likely are you to recommend our product?",
        "required": true,
        "min": 0,
        "max": 10,
        "step": 1
      },
      {
        "id": "field-4-4",
        "type": "checkbox-group",
        "label": "Which features do you use most?",
        "required": true,
        "options": [
          { "label": "Feature A", "value": "feature_a" },
          { "label": "Feature B", "value": "feature_b" },
          { "label": "Feature C", "value": "feature_c" },
          { "label": "Feature D", "value": "feature_d" }
        ]
      },
      {
        "id": "field-4-5",
        "type": "voice",
        "label": "What improvements would you like to see?",
        "placeholder": "Share your thoughts",
        "required": false
      }
    ],
    "settings": {
      "allowAnonymousResponses": true,
      "confirmationMessage": "Thank you for completing our survey!",
      "redirectUrl": "",
      "sendConfirmationEmail": false,
      "notifyOnSubmission": true
    },
    "createdAt": "2023-03-01T00:00:00.000Z",
    "updatedAt": "2023-03-01T00:00:00.000Z"
  },
